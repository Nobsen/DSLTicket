grammar org.nordakademie.mwi.tickets.Tickets with org.eclipse.xtext.common.Terminals

generate tickets "http://www.nordakademie.org/mwi/tickets/Tickets"

TicketCategory:
	'ticketCategory' name=STRING description=STRING '{'
	(flow=Flow | fields+=Field* ';') ';'
	'}';

Role:
	'role' name=STRING '{'
	(fields+=Field permission+=Permission)*';'
	'}';

enum Permission:
	read | modify | delete 
;


